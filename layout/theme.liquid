  <script>
  window.dataLayer = window.dataLayer || [];
  dataLayer.push({

      //user parameters

      'em': '{{customer.email}}',
      'fn': '{{customer.first_name}}',
      'ln': '{{customer.last_name}}',
      'ct': '{{customer_address.city}}',
      'st': '{{customer_address.province_code}}',
      'zp': '{{customer_address.zip}}',
      'ph': '{{customer_address.phone | remove: "," | remove: "-" | remove: "("| remove: ")" | remove: " " }}', //No country code
      'ltv': '{{customer.total_spent  | remove: "," }}',
      'country': '{{ customer_address.country_code }}',
      'uid': '{{customer.id}}',
   	  'currency':'{{shop.currency}}',

    
    {% if template contains 'product' %}
       {% assign geolizr_price =  product.price  %}

        'ecomm_prodid': '{{ product.id }}',
		'ecomm_prodid_first_id': '{{ product.variants.first.id }}',
		'ecomm_pagetype': 'product',
		'ecomm_totalvalue': '{{ product.price | money_without_currency | prepend: geolizr_prepend_code | append: geolizr_append_code }}',

	{% elsif template contains 'cart' %}

		{% if cart.item_count > 1 %}
			'ecomm_prodid_G': [{% for item in cart.items %}'shopify_GBP_{{ item.product_id }}_{{ item.variant.id }}'{% if forloop.last != true %},{% endif %}{% endfor %}],
		    'ecomm_prodid': [{% for item in cart.items %}'{{ item.product.id }}'{% if forloop.last != true %},{% endif %}{% endfor %}],

    {% elsif cart.item_count == 1 %}
			'ecomm_prodid_G': {% for item in cart.items %}'shopify_GBP_{{ item.product_id }}_{{ item.variant.id }}'{% endfor %},
    		'ecomm_prodid': {% for item in cart.items %}'{{ item.product.id }}'{% endfor %},
		{% else %}
			'ecomm_prodid': '',
		{% endif %}
			'ecomm_pagetype': 'cart',
			'ecomm_totalvalue': {% assign geolizr_price =  cart.total_price  %}{{ cart.total_price | money_without_currency | prepend: geolizr_prepend_code | append: geolizr_append_code }},

	{% elsif template contains 'search' %}

		'ecomm_prodid': '',
		'ecomm_pagetype': 'searchresults',
		'ecomm_totalvalue': '',

	{% elsif template contains 'index' %}

		'ecomm_prodid': '',
		'ecomm_pagetype': 'home',
		'ecomm_totalvalue': '',
{% else %}

		'ecomm_prodid': '',
		'ecomm_pagetype': 'other',
		'ecomm_totalvalue': '',

	{% endif %}

  });
  </script>
